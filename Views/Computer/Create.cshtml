@model MvcLabManager.Models.Computer

@{
    ViewData["Title"] = $"Criar novo computador";
}
<h1>@ViewData["Title"]</h1>

<form id="form" action="Create" method="post">
    <p>
        <label for="ram">Ram: </label>
        <input type="text" name="ram" id="ram" required>
    </p>

    <p>
        <label for="processor">Processador: </label>
        <input type="text" name="processor" id="processor" required>
    </p>

    <button type="submit">Criar</button>
</form>